{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": false,
        "no-empty": false,
        "max-line-length": [
            true,
            220
        ],
        "ordered-imports": false,
        "max-classes-per-file": false,
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "ban-types": [
            true,
            "Function"
        ]
    },
    "rulesDirectory": []
}